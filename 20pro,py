mok,kite=cap(int,input().split())
man=list(cap(int,input().split()))
man.sort()
man.reverse()
mm=kite
pp=0
for j in man:
    if(mm>=j):
        cat=int(mm/j)
        pp=pp+cat
        mm=mm-cat*j
    if mm==0:
        break
if(mm==0):
   print(pp)
else:
   print("it's not posiible to select coins in such a way that they sum upto",kite)
